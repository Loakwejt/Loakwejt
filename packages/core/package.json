{
  "name": "@builderly/core",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schemas": {
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.js",
      "types": "./dist/schemas/index.d.ts"
    },
    "./registry": {
      "import": "./dist/registry/index.mjs",
      "require": "./dist/registry/index.js",
      "types": "./dist/registry/index.d.ts"
    },
    "./actions": {
      "import": "./dist/actions/index.mjs",
      "require": "./dist/actions/index.js",
      "types": "./dist/actions/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/plugins/index.mjs",
      "require": "./dist/plugins/index.js",
      "types": "./dist/plugins/index.d.ts"
    },
    "./templates": {
      "import": "./dist/templates/index.mjs",
      "require": "./dist/templates/index.js",
      "types": "./dist/templates/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@builderly/config": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
